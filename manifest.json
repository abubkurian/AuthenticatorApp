{
  "manifest_version": 3,
  "name": "Mint",
  "version": "2.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/iconbuild16.png",
      "48": "icons/iconbuild48.png"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
  },
  "permissions": [
    "storage",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "*://*/*"
  ],
    "file_system_provider_capabilities": {
    "configurable": true,
    "watchable": true
  },
  "icons": {
    "16": "icons/iconbuild16.png",
    "48": "icons/iconbuild48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}