{
  "manifest_version": 3,
  "name": "Mint",
  "version": "2.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/iconbuild16.png",
      "48": "icons/iconbuild48.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "icons": {
    "16": "icons/iconbuild16.png",
    "48": "icons/iconbuild48.png",
    "128": "icons/iconbuild128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ]
}