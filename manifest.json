{
  "manifest_version": 3,
  "name": "Mint 2.0",
  "version": "2.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/iconbuild.png",
      "48": "icons/iconbuild.png"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "icons": {
    "16": "icons/iconbuild16.png",
    "48": "icons/iconbuild48.png",
    "128": "icons/icon128.png"
  }
}